<div class="container">
  <div class="login">
    <%= form_with url: { action: :login } do |f| %>
      
      <div class="new-youtube">
        <div class="new-youtube-label">
          <%= f.label :email %>
        </div>
        <div class="new-youtube-input">
          <%= f.email_field :email %>
        </div>
      </div>
      
      
      
      
      <div class="new-youtube">
        <div class="new-youtube-label">
          <%= f.label :password_confirmation %>
        </div>
        <div class="new-youtube-input">
          <%= f.password_field :password_confirmation %>
        </div>
      </div>
      
      <%= f.submit "ログイン", class: "btn submit" %>
    <% end %>
  </div>
</div>